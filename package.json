{
  "name": "vue-i18n-translator",
  "version": "0.1.0",
  "homepage": "git@github.com:f/vue-i18n-translator",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "yarn run build && push-dir --dir=dist --branch=gh-pages"
  },
  "dependencies": {
    "flat": "^4.1.0",
    "js-yaml": "^3.12.0",
    "vue": "^2.5.17",
    "vue-codemirror": "^4.0.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "push-dir": "^0.4.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
